{
  "name": "notif-cli",
  "version": "0.1.2",
  "main": "./dist/notif.js",
  "bin": {
    "notif": "dist/src/main.js"
  },
  "keywords": [],
  "author": "jdjfisher",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register -r dotenv/config src/notif.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write 'src/**/*.ts'",
    "clean": "rm -rf ./dist ~/.notif/settings.json",
    "check": "tsc --noEmit",
    "test": "tsc && jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "dist/src/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.4",
    "commander": "^8.2.0",
    "qrcode": "^1.4.4",
    "socket.io-client": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.1",
    "@types/qrcode": "^1.4.1",
    "@types/socket.io-client": "^3.0.0",
    "babel-jest": "^27.3.1",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.5"
  }
}
